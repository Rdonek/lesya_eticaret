{
  "expo": {
    "name": "Lesya",
    "slug": "lesya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/lesya_favicon/android/play_store_512.png",
    "scheme": "lesya",
    "userInterfaceStyle": "dark",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lesyastudio.mobile",
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/lesya_favicon/android/res/mipmap-xxxhdpi/lesya_launcher_foreground.png",
        "backgroundImage": "./assets/lesya_favicon/android/res/mipmap-xxxhdpi/lesya_launcher_background.png",
        "monochromeImage": "./assets/lesya_favicon/android/res/mipmap-xxxhdpi/lesya_launcher_monochrome.png",
        "backgroundColor": "#000000"
      },
      "icon": "./assets/lesya_favicon/android/play_store_512.png",
      "package": "com.lesyastudio.mobile",
      "edgeToEdgeEnabled": true,
      "backgroundColor": "#000000",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/lesya_favicon/Lesya_nobg.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/lesya_favicon/Lesya_nobg.png",
          "imageWidth": 120,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/lesya_favicon/Lesya_nobg.png"
          }
        }
      ],
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/lesya_favicon/android/res/mipmap-xxxhdpi/lesya_launcher_monochrome.png",
          "color": "#000000",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e5336a34-0f0a-47c1-a365-ba7d99d34e3f"
      }
    },
    "owner": "rdonek"
  }
}
